        "@ng-web-apis/audio": "latest",
        "@ng-web-apis/common": "latest",
        "@ng-web-apis/geolocation": "latest",
        "@ng-web-apis/intersection-observer": "latest",
        "@ng-web-apis/midi": "latest",
        "@ng-web-apis/payment-request": "latest",
